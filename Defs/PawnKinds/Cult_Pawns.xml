<Defs>

  
  <PawnKindDef Name="CultistBase" Abstract="True">
    <race>Human</race>
    <defaultFactionType>EtherCult</defaultFactionType>
    <maxGenerationAge>80</maxGenerationAge>
    <backstoryCryptosleepCommonality>0.08</backstoryCryptosleepCommonality>
    <backstoryFiltersOverride>
      <li>
        <categories>
          <li>ImperialRoyal</li>
        </categories>
      </li>
    </backstoryFiltersOverride>
    <royalTitleChance>1</royalTitleChance>
    <baseRecruitDifficulty>0.90</baseRecruitDifficulty>
    <chemicalAddictionChance>0.05</chemicalAddictionChance>
    <itemQuality>Excellent</itemQuality>
    <combatPower>150</combatPower>
    <weaponMoney>2600~3100</weaponMoney>
    <weaponTags>
      <li>Gun</li>
      <li>MedievalMeleeBasic</li>
    </weaponTags>
    <apparelAllowHeadgearChance>0.5</apparelAllowHeadgearChance>
    <apparelMoney>1000~1500</apparelMoney>
    <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
    <apparelTags>
      <li>IndustrialBasic</li>
    </apparelTags>
    <apparelDisallowTags>
      <li>Western</li>
    </apparelDisallowTags>
    <techHediffsChance>0.15</techHediffsChance>
    <techHediffsMoney>500~1500</techHediffsMoney>
    <techHediffsTags>
      <li>Advanced</li>
    </techHediffsTags>
    <techHediffsDisallowTags>
      <li>PainCauser</li>
    </techHediffsDisallowTags>
    <hairTags>
      <li>Royalty</li>
    </hairTags>
    <disallowedTraits>
      <li>Nudist</li>
    </disallowedTraits>
	<!-- TODO: ADD CULTIST MUTATIONS WHEN THOSE ARE DONE-->
	<modExtensions>
      <li Class="Pawnmorph.MorphPawnKindExtension">
        <morphs>
            <li>BoarMorph</li>
			<li>CatMorph</li>
			<li>FoxMorph</li>
			<li>CowMorph</li>
			<li>HuskyMorph</li>
			<li>ChookMorph</li>
			<li>RatMorph</li>
			<li>PigMorph</li>
			<li>DeerMorph</li>
			<li>BearMorph</li>
			<li>MonkeyMorph</li>
			<li>WargMorph</li>
			<li>AlpacaMorph</li>
			<li>WolfMorph</li>
        </morphs>
        <hediffRange>0~10</hediffRange>
      </li>
    </modExtensions>
  </PawnKindDef>

  <PawnKindDef ParentName="CultistBase">
    <defName>EtherCult_Ranked_Wimp</defName>
    <label>ether priest</label>
    <titleSelectOne>
      <li>Follower</li>
      <li>Acolyte</li>
      <li>Disciple</li>
      <li>Preacher</li>
      <li>Elder</li>
    </titleSelectOne>
    <specificApparelRequirements>
      <li>
        <bodyPartGroup>FullHead</bodyPartGroup>
        <requiredTag>EtherTier2</requiredTag>
      </li>
      <li>
        <bodyPartGroup>Torso</bodyPartGroup>
        <apparelLayer>Shell</apparelLayer>
        <requiredTag>EtherTier2</requiredTag>
      </li>
      <li>
        <bodyPartGroup>Torso</bodyPartGroup>
        <apparelLayer>Middle</apparelLayer>
        <requiredTag>RoyalTier5</requiredTag>
      </li>
      <li>
        <bodyPartGroup>Torso</bodyPartGroup>
        <apparelLayer>OnSkin</apparelLayer>
        <requiredTag>RoyalTier2</requiredTag>
      </li>
    </specificApparelRequirements>
    <weaponTags Inherit="False" />
    <techHediffsMoney>0</techHediffsMoney>
    <disallowedTraits>
      <li>Tough</li>
    </disallowedTraits>
    <allowRoyalRoomRequirements>False</allowRoyalRoomRequirements>
    <allowRoyalApparelRequirements>False</allowRoyalApparelRequirements>
  </PawnKindDef>
  
