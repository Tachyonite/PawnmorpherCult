<Defs>
	<Pawnmorph.Hediffs.MutationDef ParentName="PawnmorphPart">
		<defName>EtherMaterialTalons</defName>
		<label>talons</label>
		<description>The fingernails on this hand have grown out into talons.</description>
		<parts>
			<li>Hand</li>
		</parts>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
                <minSeverity>0</minSeverity> <!-- only pawns with the Paragon aspect can get this stage -->
                <labelOverride>bone talons</labelOverride>
            </li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
                <minSeverity>0.2</minSeverity> <!-- only pawns with the Paragon aspect can get this stage -->
                <labelOverride>stone talons</labelOverride>
            </li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
                <minSeverity>0.4</minSeverity> <!-- only pawns with the Paragon aspect can get this stage -->
                <labelOverride>metal talons</labelOverride>
                <partEfficiencyOffset>0.1</partEfficiencyOffset>
            </li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
                <minSeverity>0.6</minSeverity> <!-- only pawns with the Paragon aspect can get this stage -->
                <labelOverride>vine talons</labelOverride>
            </li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
                <minSeverity>0.8</minSeverity> <!-- only pawns with the Paragon aspect can get this stage -->
                <labelOverride>crystal talons</labelOverride>
            </li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
                <minSeverity>1</minSeverity> <!-- only pawns with the Paragon aspect can get this stage -->
                <labelOverride>ethereal talons</labelOverride>
            </li>
		</stages>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>talons</label>
						<labelUsedInLogging>False</labelUsedInLogging>
						<capacities>
							<li>Scratch</li>
						</capacities>
						<power>13.0</power>
						<cooldownTime>1.5</cooldownTime>
						<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
						<surpriseAttack>
							<extraMeleeDamages>
								<li>
									<def>Stun</def>
									<amount>14</amount>
								</li>
							</extraMeleeDamages>
						</surpriseAttack>
					</li>
				</tools>
			</li>
		</comps>
	</Pawnmorph.Hediffs.MutationDef>
	
