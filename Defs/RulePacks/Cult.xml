      <li>CultNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=4)->[townname_wordgen]</li>
        <li>r_name->[townname_fixed]</li>

        <li>townname_wordgen->[WordCultist]</li>
        <li>townname_wordgen->[WordCultist] [churchPlace]</li>
        <li>townname_wordgen->[WordCultist][townend]</li>
		
