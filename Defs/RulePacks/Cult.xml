  <RulePackDef>
    <defName>NamerPersonEtherCult</defName>
    <include>
      <li>CultNameUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=3)->Brother&#160;[EtherealNameFirstMale] [EtherealNameLast]</li>
        <li>r_name(p=2)->Brother&#160;[EtherealNameFirstMale] [WordCultist]</li>
        <li>r_name(p=2)->Brother&#160;[WordCultist] [WordCultist]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

