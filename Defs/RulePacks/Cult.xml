<RulePackDef>
    <defName>CultNameUtility</defName>
    <rulePack>
      <rulesStrings>
        <li>WordCultist(p=4)->[SylCult][SylCult]</li>
        <li>WordCultist(p=2)->[SylCult][SylCult][SylCult]</li>
        <li>WordCultist(p=0.2)->[SylCult][SylCult][SylCult][SylCult]</li>
      </rulesStrings>
      <rulesFiles>
        <li>EtherealNameFirstMale->Names/Ethereal_First_Male</li>
        <li>EtherealNameFirstFemale->Names/Ethereal_First_Female</li>
        <li>EtherealNameLast->Names/Ethereal_Last</li>
		<li>SylCult->Names/Cult_Syllables</li>
